{
  "name": "keduije",
  "version": "1.0.0",
  "description": "NLP for igbo language",
  "repository": {
    "type": "git",
    "url": "git://github.com/nnennaude/keduije.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc mocha test/**/*.spec.js --compilers js:babel-register",
    "test-debug": "DEBUG=express:* NODE_ENV=test mocha --debug-brk --inspect test/**/*.spec.js --compilers js:babel-register",
    "test-browser": "webpack --env=test && npm run start-dev",
    "start-debug": "babel-node --debug-brk --inspect lib/main.js",
    "build-front": "webpack --env=prod",
    "build-back": "babel lib -d dist",
    "build": "npm run build-front && npm run build-back",
    "heroku-postbuild": "npm run build",
    "start-dev": "nodemon lib/main.js --exec babel-node ",
    "start": "node dist/main.js"
  },
  "author": "Udegbunam, Nnenna",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.16.1",
    "bootstrap": "^3.3.7",
    "connect-ensure-login": "^0.1.1",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "diff": "^3.2.0",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "jquery": "^3.2.1",
    "jquery-ui": "^1.12.1",
    "mongodb": "^2.2.24",
    "morgan": "^1.8.1",
    "nodemailer": "^3.1.8",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "path": "^0.12.7",
    "prop-types": "^15.5.4",
    "pug": "^2.0.0-beta11",
    "react": "^15.5.3",
    "react-addons-css-transition-group": "^15.5.1",
    "react-addons-update": "^15.5.1",
    "react-dom": "^15.5.3",
    "slugify": "^1.1.0",
    "webpack": "^2.3.3"
  },
  "bugs": {
    "url": "https://github.com/nnennaude/keduije/issues"
  },
  "homepage": "https://github.com/nnennaude/keduije#readme",
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-rewire": "^1.1.0",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "cross-env": "^4.0.0",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.2.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.1.0",
    "supertest": "^3.0.0"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
